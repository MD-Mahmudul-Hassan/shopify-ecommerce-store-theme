      $(function() {
        $('.mobile-nav > li').click(function(e) {
          e.stopPropagation();
          var $el = $('ul',this);
          $('.mobile-nav > li > ul').not($el).slideUp();
          $el.stop(true, true).slideToggle(400);
          if ($(this).hasClass("open")) {
            $(this).removeClass("open");
          }
          else
          {
            $(this).addClass("open");
          }
        });
        $('.mobile-nav > li > ul > li').click(function(e) {
          e.stopImmediatePropagation();  
        });


        jQuery('<span class="size_span"><a href="#" id="size_span">Size guide</a></span>').insertAfter('.product-form__cart-submit');

        jQuery(".site-nav__childlist-grid").append('<li class="slider-image1 slideswidth"><a href="/collections/all"><img src="{{ 'menu_image.jpg' | file_url }}"></a></li>');

                                                   jQuery('li.site-nav--has-centered-dropdown').hover(function() {
          if (jQuery(this).hasClass('site-nav--active-dropdown')){
            jQuery(this).removeClass('site-nav--active-dropdown');
            $(this).children('.site-nav__dropdown').css('display', 'none');
          }
          else{
            jQuery(this).addClass('site-nav--active-dropdown');
            $(this).children('.site-nav__dropdown').css('display', 'block');
          }  
        });

      });


    </script>

    <script>
      function sticky_relocate() {
        var window_top = jQuery(window).scrollTop();
        var footer_top = jQuery("#shopify-section-product-recommendations").offset().top;
        var div_top = jQuery('#before-sticky-sidebar').offset().top + jQuery('#before-sticky-sidebar').height();
        var div_height = jQuery("#prod_description").height();

        var padding = 20;  

        if (window_top + div_height > footer_top - padding) {
          var toplength = $('#shopify-section-product-template').innerHeight() - $('#prod_description').innerHeight() - 50;
          $('#prod_description').css({'position': 'absolute','top': toplength });
          //jQuery('#prod_description').css({top: (window_top + div_height - footer_top + padding) * -1});
          jQuery('#prod_description').removeClass('stick');
        }
        else if (window_top > div_top) {
          jQuery('#prod_description').addClass('stick');
          jQuery('#prod_description').css({top: 0,'position': 'fixed'});
        } else {
          $('#prod_description').css({'position': 'absolute','top': '75px' });
          jQuery('#prod_description').removeClass('stick');
        }
      }

      jQuery(document).ready(function() {
        if ($(window).width() > 768) {
          jQuery(window).scroll(sticky_relocate);
          sticky_relocate();
        }
        else{
          var screenHeight = $(window).height() - 150;
          $('.mobile-nav-wrapper').css('height',screenHeight+'px');
//         	alert(screenHeight);
        }
      });
    </script>

    <script>
      $(function() {
        $('.js-mobile-nav-toggle').on('click', function() {
          $('body').toggleClass('menu-open');
        });


        $('#size_span').on('click', function() {
          $('#size-chart-modal').css("display","block");
          $('body.template-product').addClass("modal-open");
        });

        $('span.close').on('click', function() {
          $('#size-chart-modal').css("display","none");
          $('body.template-product').removeClass("modal-open");
        });


        $('.info-icon').on('click', function() {
          $('#afterpay-modal-overlay').css("display","block");
          $('body.template-product').addClass("afterpay-modal-open");
        });

        $('#afterpay-modal-close').on('click', function() {
          $('#afterpay-modal-overlay').css("display","none");
          $('body.template-product').removeClass("afterpay-modal-open");
        });
      });

      jQuery('.single-option-selector.option_Size').on('change', function() {
        jQuery('.first_price').hide();
        if(jQuery(this).children("option:selected").val() == '')
        {
          jQuery('.first_price').show();
        }
      });


    </script>


