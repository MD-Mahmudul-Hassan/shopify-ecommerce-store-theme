<div class="about-page">
<div class="page-width about">
  <div class="grid">
    <div class="grid__item medium-up--one-half content">
      <div class="section-header text-left">
        <h1>{{ page.title }}</h1>
      </div>

      <div class="rte">
        {{ page.content }}
      </div>
    </div>
     <div class="grid__item medium-up--one-half image">
      <div class="section-header text-center">
        <img src="{{ "about_img.jpg" | file_url }}" alt="about_image">
      </div>
    </div>
  </div>
</div>
</div>
